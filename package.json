{
  "name": "r2-form",
  "version": "1.5.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack",
    "prepublish": "npm run production",
    "production": "webpack -p",
    "test": "jest; exit 0",
    "test:watch": "jest --watch",
    "test:watchall": "jest --watchAll",
    "test:cov": "jest --coverage; exit 0",
    "lint": "eslint \"src/**/*.{js,jsx}\" || true",
    "format": "prettier-eslint --single-quote --trailing-comma all --no-semi --write \"src/**/*.{js,jsx}\" && npm run lint",
    "precommit": "npm run format; git add ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blueqaus/r2-form.git"
  },
  "keywords": [
    "babel",
    "blueq",
    "form",
    "immutablejs",
    "javascript",
    "react",
    "react-redux",
    "redux"
  ],
  "author": "Rhys Gevaux <rhys.gevaux@blueq.com.au>",
  "contributors": [
    {
      "name": "Rhys Gevaux",
      "url": "https://twitter.com/vorksau"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blueqaus/r2-form/issues"
  },
  "homepage": "https://github.com/blueqaus/r2-form#readme",
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es)"
    ]
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "lodash-es": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.0",
    "redux-actions": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-blueq": "^2.0.2",
    "eslint-config-prettier": "^2.2.0",
    "eslint-import-resolver-webpack": "^0.8.2",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-immutablejs": "^0.1.3",
    "eslint-plugin-import": "^2.5.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.42.0",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "jest-immutable-matchers": "^1.5.0",
    "lodash-webpack-plugin": "^0.11.4",
    "prettier": "^1.4.4",
    "prettier-eslint-cli": "^4.1.1",
    "react-dom": "^15.6.1",
    "webpack": "^2.6.1",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
